        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH last_num_cpus AS\n(\n  select\n  \tdistinct on (e.oid)\n  \te.oid,\n  \te.name,\n  \t(e.attrs->>'num_cpus')::integer as \"num_cpus\"\n  from entity e\n  where\n  \te.\"type\" = 'VIRTUAL_MACHINE'\n    AND e.oid IN (${VM_OID})\n  order by e.oid, e.last_seen DESC\n),\navg_num_cpu as\n(\n\tselect\n\t\thea.entity_oid as oid,\n\t\tavg(hea.int_value) as \"num_cpus\"\n\tfrom historical_entity_attrs hea\n\tWHERE\n  \t\t$__timeFilter(hea.time)\n  \t    AND hea.type = 'NUM_VCPU'\n        AND hea.entity_oid IN (${VM_OID})\n  \tgroup by hea.entity_oid \n)\nSELECT\n    lnc.name as \"VM Name\",\n    coalesce(a.num_cpus, lnc.num_cpus) as \"Average number of vCPU(s)\",\n    ${VCPU_COST} as \"vCPU Price ($)\"\nfrom last_num_cpus lnc\nleft join avg_num_cpu a on a.oid = lnc.oid",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "savings"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "completed_action",
          "timeColumn": "recommendation_time",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        },
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  e.name as \"VM Name\",\n--  m.type,\n  avg(m.capacity)/(1024*1024) as \"vMem Average (GB)\",\n  ${VMEM_COST} as \"GB vMem Price ($)\"\nFROM metric m\nJOIN entity e ON e.oid = m.entity_oid\nWHERE\n  $__timeFilter(time)\n  AND entity_oid IN (${VM_OID})\n  AND m.type = 'VMEM'\nGROUP BY e.name, m.type\nORDER BY 1",
          "refId": "B",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        },
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  e.name as \"VM Name\",\n--  m.type,\n  avg(m.consumed)/(1024) as \"Storage Average (GB)\",\n  ${STORAGE_COST} as \"GB Storage Price ($)\"\nFROM metric m\nJOIN entity e ON e.oid = m.entity_oid\nWHERE\n  $__timeFilter(time)\n  AND entity_oid IN (${VM_OID})\n  AND m.type = 'STORAGE_PROVISIONED'\nGROUP BY e.name, m.type\nORDER BY 1",
          "refId": "C",
